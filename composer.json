{
  "name": "iampersistent/simple-shop",
  "type": "library",
  "description": "A small feature set ecommerce library",
  "keywords": ["ecommerce"],
  "license": "MIT",
  "require": {
    "ext-pdo": "*",
    "moneyphp/money": "^3.0@dev",
    "php": ">=7.2",
    "psr/container": "^1.0@dev",
    "league/omnipay": "^3",
    "robmorgan/phinx": "dev-master",
    "doctrine/dbal": "^2.9@dev"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "ext-yaml": "*"
  },
  "repositories": {
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "IamPersistent\\SimpleShop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixture\\": "tests/_support/Fixture",
      "Tests\\Functional\\": "tests/functional",
      "Tests\\Unit\\": "tests/unit"
    }
  }
}
